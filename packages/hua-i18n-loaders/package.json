{
  "name": "@hua-labs/i18n-loaders",
  "version": "2.0.4",
  "description": "Production-ready loaders, caching, and preloading helpers for @hua-labs/i18n-core.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "type-check": "tsc --noEmit"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@hua-labs/i18n-core": "workspace:*"
  },
  "peerDependencies": {
    "react": ">=19.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.2.0",
    "@types/react": "^19.2.14",
    "eslint": "^10.0.0",
    "react": "^19.2.4",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "keywords": [
    "hua-labs",
    "i18n",
    "internationalization",
    "translation",
    "loader",
    "preload",
    "cache",
    "react",
    "typescript"
  ],
  "author": "HUA Labs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/HUA-Labs/HUA-Labs-public.git"
  },
  "bugs": {
    "url": "https://github.com/HUA-Labs/HUA-Labs-public/issues"
  },
  "homepage": "https://github.com/HUA-Labs/HUA-Labs-public#readme",
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}