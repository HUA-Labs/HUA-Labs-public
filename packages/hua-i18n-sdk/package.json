{
  "name": "@hua-labs/i18n-sdk",
  "publishConfig": {
    "access": "public"
  },
  "version": "1.3.0",
  "description": "A lightweight and powerful internationalization SDK for React applications, inspired by hua-api's translation system",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./easy": {
      "types": "./dist/easy.d.ts",
      "import": "./dist/easy.js",
      "require": "./dist/easy.js"
    },
    "./simple": {
      "types": "./dist/simple.d.ts",
      "import": "./dist/simple.js",
      "require": "./dist/simple.js"
    },
    "./beginner": {
      "types": "./dist/beginner.d.ts",
      "import": "./dist/beginner.js",
      "require": "./dist/beginner.js"
    },
    "./advanced": {
      "types": "./dist/advanced.d.ts",
      "import": "./dist/advanced.js",
      "require": "./dist/advanced.js"
    },
    "./debug": {
      "types": "./dist/debug.d.ts",
      "import": "./dist/debug.js",
      "require": "./dist/debug.js"
    },
    "./core": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.js",
      "require": "./dist/core.js"
    },
    "./plugins": {
      "types": "./dist/plugins.d.ts",
      "import": "./dist/plugins.js",
      "require": "./dist/plugins.js"
    },
    "./ai": {
      "types": "./dist/ai.d.ts",
      "import": "./dist/ai.js",
      "require": "./dist/ai.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "README.en.md",
    "docs/CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "i18n",
    "internationalization",
    "translation",
    "react",
    "typescript",
    "multilingual",
    "localization",
    "ssr",
    "hooks",
    "deprecated"
  ],
  "author": "HUA Team",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.0.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.0.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "commander": "^11.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HUA-Labs/i18n-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/HUA-Labs/i18n-sdk/issues"
  },
  "homepage": "https://github.com/HUA-Labs/i18n-sdk#readme",
  "deprecated": "This package is deprecated. Please migrate to domain-specific packages: @hua-labs/i18n-beginner, @hua-labs/i18n-advanced, @hua-labs/i18n-core, etc. See: https://github.com/HUA-Labs/hua-platform#migration"
}
