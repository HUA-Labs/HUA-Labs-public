{
  "name": "@hua-labs/ui",
  "version": "1.1.0-alpha.1",
  "description": "HUA Labs - Modern React UI Component Library - Beautiful, accessible, and customizable components for React applications",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./advanced": {
      "types": "./dist/advanced.d.ts",
      "import": "./dist/advanced.mjs",
      "require": "./dist/advanced.js"
    },
    "./advanced/dashboard": {
      "types": "./dist/advanced-dashboard.d.ts",
      "import": "./dist/advanced-dashboard.mjs",
      "require": "./dist/advanced-dashboard.js"
    },
    "./advanced/motion": {
      "types": "./dist/advanced-motion.d.ts",
      "import": "./dist/advanced-motion.mjs",
      "require": "./dist/advanced-motion.js"
    },
    "./advanced/emotion": {
      "types": "./dist/advanced-emotion.d.ts",
      "import": "./dist/advanced-emotion.mjs",
      "require": "./dist/advanced-emotion.js"
    },
    "./form": {
      "types": "./dist/form.d.ts",
      "import": "./dist/form.mjs",
      "require": "./dist/form.js"
    },
    "./navigation": {
      "types": "./dist/navigation.d.ts",
      "import": "./dist/navigation.mjs",
      "require": "./dist/navigation.js"
    },
    "./feedback": {
      "types": "./dist/feedback.d.ts",
      "import": "./dist/feedback.mjs",
      "require": "./dist/feedback.js"
    },
    "./sdui": {
      "types": "./dist/sdui.d.ts",
      "import": "./dist/sdui.mjs",
      "require": "./dist/sdui.js"
    },
    "./components/*": {
      "types": "./dist/components/*.d.ts",
      "import": "./dist/components/*.mjs",
      "require": "./dist/components/*.js"
    },
    "./components/advanced/*": {
      "types": "./dist/components/advanced/*.d.ts",
      "import": "./dist/components/advanced/*.mjs",
      "require": "./dist/components/advanced/*.js"
    },
    "./styles/toast.css": "./src/styles/toast.css",
    "./styles/codeblock.css": "./src/styles/codeblock.css",
    "./styles/recommended-theme.css": "./src/styles/recommended-theme.css"
  },
  "files": [
    "dist",
    "src/styles"
  ],
  "scripts": {
    "build": "pnpm exec tsup && pnpm exec tsc --emitDeclarationOnly && node scripts/add-use-client.js",
    "build:watch": "pnpm exec tsup --watch",
    "dev": "pnpm exec tsup --watch",
    "clean": "rm -rf dist",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "analyze": "node scripts/analyze-bundle.js",
    "build:analyze": "pnpm build && pnpm analyze",
    "lint": "eslint .",
    "lint:strict": "eslint . --max-warnings 0",
    "type-check": "tsc --noEmit",
    "generate:icon-types": "tsx scripts/generate-icon-types.ts",
    "generate:icon-snippets": "tsx scripts/generate-icon-snippets.ts"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "clsx": "^2.0.0",
    "lucide-react": "^0.562.0",
    "sugar-high": "^0.9.5",
    "tailwind-merge": "^2.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@phosphor-icons/react": "^2.1.10",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.0.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.3.0",
    "jsdom": "^27.2.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "rollup-plugin-visualizer": "^6.0.5",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.35.1",
    "vitest": "^4.0.14"
  },
  "peerDependencies": {
    "@hua-labs/motion-core": "^2.1.0-alpha.0",
    "@phosphor-icons/react": "*",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "keywords": [
    "hua-labs",
    "ui-library",
    "react-components",
    "react",
    "typescript",
    "design-system",
    "accessibility",
    "aria",
    "dark-mode",
    "tailwind",
    "nextjs",
    "ssr"
  ],
  "author": "HUA Labs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/HUA-Labs/HUA-Labs-public.git"
  },
  "bugs": {
    "url": "https://github.com/HUA-Labs/HUA-Labs-public/issues"
  },
  "homepage": "https://github.com/HUA-Labs/HUA-Labs-public#readme",
  "sideEffects": [
    "**/*.css"
  ]
}