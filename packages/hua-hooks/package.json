{
  "name": "@hua-labs/hooks",
  "version": "1.0.0",
  "description": "공통 React 훅 모음",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "node ../../node_modules/typescript/lib/tsc.js",
    "dev": "node ../../node_modules/typescript/lib/tsc.js --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/react": "^19.2.10",
    "jsdom": "^27.4.0",
    "react": "^19.2.4",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "react": ">=19.0.0"
  },
  "keywords": [
    "react",
    "hooks",
    "custom-hooks",
    "typescript"
  ],
  "author": "HUA Labs",
  "license": "MIT",
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}