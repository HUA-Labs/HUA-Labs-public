{
  "name": "@hua-labs/hooks",
  "version": "1.1.0",
  "description": "Common React hooks for the hua ecosystem",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {},
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.2.10",
    "jsdom": "^27.4.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "react": ">=19.0.0"
  },
  "keywords": [
    "react",
    "hooks",
    "custom-hooks",
    "typescript"
  ],
  "author": "HUA Labs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/HUA-Labs/HUA-Labs-public.git"
  },
  "bugs": {
    "url": "https://github.com/HUA-Labs/HUA-Labs-public/issues"
  },
  "homepage": "https://github.com/HUA-Labs/HUA-Labs-public#readme",
  "sideEffects": false,
  "engines": {
    "node": ">=20.0.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}