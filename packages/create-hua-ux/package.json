{
  "name": "create-hua-ux",
  "version": "1.0.0-alpha.25",
  "description": "Scaffolding tool for creating hua-ux projects",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "create-hua-ux": "./dist/bin/create-hua-ux.js"
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "prebuild": "tsx scripts/generate-version.ts && tsx scripts/validate-template.ts",
    "build": "node ../../node_modules/typescript/lib/tsc.js",
    "dev": "tsx src/index.ts",
    "test": "tsx src/index.ts test-project",
    "test:cli": "bash scripts/test-cli.sh",
    "validate:template": "tsx scripts/validate-template.ts",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "fs-extra": "^11.2.0",
    "inquirer": "^8.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^25.0.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "hua-labs",
    "hua-ux",
    "scaffolding",
    "cli",
    "create-app",
    "boilerplate",
    "nextjs",
    "react",
    "typescript",
    "starter"
  ],
  "author": "HUA Labs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/HUA-Labs/HUA-Labs-public.git"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
