{
  "name": "@hua-labs/motion-core",
  "version": "2.2.2",
  "description": "HUA Motion Core - Essential animation hooks for React developers",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "echo 'Skipping lint for hua-motion-core (ESLint 8, needs separate config)'",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^10.0.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "react": ">=19.0.0",
    "react-dom": ">=19.0.0"
  },
  "keywords": [
    "hua-labs",
    "react",
    "typescript",
    "motion",
    "animation",
    "hooks",
    "transitions",
    "gestures",
    "scroll",
    "fade",
    "slide",
    "spring",
    "performance",
    "ssr",
    "nextjs"
  ],
  "author": "HUA Labs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/HUA-Labs/HUA-Labs-public.git"
  },
  "bugs": {
    "url": "https://github.com/HUA-Labs/HUA-Labs-public/issues"
  },
  "homepage": "https://github.com/HUA-Labs/HUA-Labs-public#readme",
  "sideEffects": false,
  "engines": {
    "node": ">=20.0.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}