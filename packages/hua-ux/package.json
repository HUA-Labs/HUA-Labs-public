      "types": "./dist/state/index.d.mts",
      "import": "./dist/state/index.mjs",
      "require": "./dist/state/index.js",
      "default": "./dist/state/index.mjs"
    },
    "./pro": {
      "types": "./dist/pro/index.d.mts",
      "import": "./dist/pro/index.mjs",
      "require": "./dist/pro/index.js",
      "default": "./dist/pro/index.mjs"
    },
    "./formatters": {
      "types": "./dist/formatters/index.d.mts",
      "import": "./dist/formatters/index.mjs",
      "require": "./dist/formatters/index.js",
      "default": "./dist/formatters/index.mjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.mts",
      "import": "./dist/utils/index.mjs",
      "require": "./dist/utils/index.js",
      "default": "./dist/utils/index.mjs"
    },
    "./hooks": {
      "types": "./dist/hooks/index.d.mts",
      "import": "./dist/hooks/index.mjs",
      "require": "./dist/hooks/index.js",
      "default": "./dist/hooks/index.mjs"
    },
    "./loaders": {
      "types": "./dist/loaders/index.d.mts",
      "import": "./dist/loaders/index.mjs",
      "require": "./dist/loaders/index.js",
      "default": "./dist/loaders/index.mjs"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "tsup && node scripts/add-use-client.mjs",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "lint": "echo 'Skipping lint for hua-ux (ESLint 8, needs separate config)'",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "pnpm run build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@hua-labs/hooks": "workspace:*",
    "@hua-labs/i18n-core": "workspace:*",
    "@hua-labs/i18n-core-zustand": "workspace:*",
    "@hua-labs/i18n-formatters": "workspace:*",
    "@hua-labs/i18n-loaders": "workspace:*",
