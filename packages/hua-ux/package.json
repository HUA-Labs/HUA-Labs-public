{
  "name": "@hua-labs/hua-ux",
  "version": "0.1.0-alpha.12",
  "description": "HUA UX - Ship UX faster: UI + motion + i18n, pre-wired. A framework for React product teams.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.mjs"
    },
    "./framework": {
      "types": "./dist/framework/index.d.mts",
      "import": "./dist/framework/index.mjs",
      "require": "./dist/framework/index.js",
      "default": "./dist/framework/index.mjs"
    },
    "./framework/shared": {
      "types": "./dist/framework/shared.d.mts",
      "import": "./dist/framework/shared.mjs",
      "require": "./dist/framework/shared.js",
      "default": "./dist/framework/shared.mjs"
    },
    "./framework/server": {
      "types": "./dist/framework/server.d.mts",
      "import": "./dist/framework/server.mjs",
      "require": "./dist/framework/server.js",
      "default": "./dist/framework/server.mjs"
    },
    "./framework/config": {
      "types": "./dist/framework/config.d.mts",
      "import": "./dist/framework/config.mjs",
      "require": "./dist/framework/config.js",
      "default": "./dist/framework/config.mjs"
    },
    "./presets": {
      "types": "./dist/presets/index.d.mts",
      "import": "./dist/presets/index.mjs",
      "require": "./dist/presets/index.js",
      "default": "./dist/presets/index.mjs"
    },
    "./framework/seo/geo": {
      "types": "./dist/framework/seo/geo/index.d.mts",
      "import": "./dist/framework/seo/geo/index.mjs",
      "require": "./dist/framework/seo/geo/index.js",
      "default": "./dist/framework/seo/geo/index.mjs"
    },
    "./ui": {
      "types": "./dist/ui/index.d.mts",
      "import": "./dist/ui/index.mjs",
      "require": "./dist/ui/index.js",
      "default": "./dist/ui/index.mjs"
    },
    "./i18n": {
      "types": "./dist/i18n/index.d.mts",
      "import": "./dist/i18n/index.mjs",
      "require": "./dist/i18n/index.js",
      "default": "./dist/i18n/index.mjs"
    },
    "./motion": {
      "types": "./dist/motion/index.d.mts",
      "import": "./dist/motion/index.mjs",
      "require": "./dist/motion/index.js",
      "default": "./dist/motion/index.mjs"
    },
