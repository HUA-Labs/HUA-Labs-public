{
  "name": "@hua-labs/hua-ux",
  "version": "0.1.0-alpha.12",
  "description": "HUA UX - Ship UX faster: UI + motion + i18n, pre-wired. A framework for React product teams.",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./framework": {
      "types": "./dist/framework/index.d.mts",
      "import": "./dist/framework/index.js",
      "require": "./dist/framework/index.cjs",
      "default": "./dist/framework/index.js"
    },
    "./framework/server": {
      "types": "./dist/framework/server.d.mts",
      "import": "./dist/framework/server.js",
      "require": "./dist/framework/server.cjs",
      "default": "./dist/framework/server.js"
    },
    "./framework/config": {
      "types": "./dist/framework/config.d.mts",
      "import": "./dist/framework/config.js",
      "require": "./dist/framework/config.cjs",
      "default": "./dist/framework/config.js"
    },
    "./presets": {
      "types": "./dist/presets/index.d.mts",
      "import": "./dist/presets/index.js",
      "require": "./dist/presets/index.cjs",
      "default": "./dist/presets/index.js"
    },
    "./framework/seo/geo": {
      "types": "./dist/framework/seo/geo/index.d.mts",
      "import": "./dist/framework/seo/geo/index.js",
      "require": "./dist/framework/seo/geo/index.cjs",
      "default": "./dist/framework/seo/geo/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "tsup && node scripts/add-use-client.mjs",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "lint": "echo 'Skipping lint for hua-ux (ESLint 8, needs separate config)'",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "pnpm run build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@hua-labs/i18n-core": "workspace:*",
    "@hua-labs/i18n-core-zustand": "workspace:*",
    "@hua-labs/motion-advanced": "workspace:*",
    "@hua-labs/motion-core": "workspace:*",
    "@hua-labs/state": "workspace:*",
    "@hua-labs/ui": "workspace:*"
  },
  "peerDependencies": {
    "next": ">=13.0.0",
    "react": ">=19.0.0",
    "react-dom": ">=19.0.0",
    "server-only": "^0.0.1"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    }
  },
  "keywords": [
    "hua-labs",
    "hua-ux",
    "react",
    "framework",
    "ui-components",
    "motion",
    "animation",
    "i18n",
    "internationalization",
    "typescript",
    "nextjs",
    "ssr",
    "zustand"
  ],
  "author": "HUA Labs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/HUA-Labs/HUA-Labs-public.git"
  },
  "bugs": {
    "url": "https://github.com/HUA-Labs/HUA-Labs-public/issues"
  },
  "homepage": "https://github.com/HUA-Labs/HUA-Labs-public#readme",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "jsdom": "^27.2.0",
    "server-only": "^0.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  }
}