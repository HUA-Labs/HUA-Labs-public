{
  "name": "@hua-labs/eslint-plugin-i18n",
  "version": "0.1.0",
  "description": "ESLint plugin for i18n type safety â€” missing keys, unused keys, raw text, dynamic keys",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/estree": "^1.0.7",
    "@types/node": "^25.2.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "i18n",
    "internationalization",
    "hua-labs"
  ],
  "author": "HUA Labs",
  "license": "MIT"
}
