    "@types/node": "^25.2.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^9.21.0",
    "eslint-plugin-react": "^7.37.5",
    "espree": "^11.1.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "i18n",
    "internationalization",
    "hua-labs"
  ],
  "author": "HUA Labs",
  "license": "MIT",
  "repository": {
    "type": "git",
