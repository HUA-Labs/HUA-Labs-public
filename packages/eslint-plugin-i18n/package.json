{
  "name": "@hua-labs/eslint-plugin-i18n",
  "version": "0.1.1",
  "description": "ESLint plugin for i18n type safety â€” missing keys, unused keys, raw text, dynamic keys",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.6",
    "@babel/preset-react": "^7.28.5",
