{
  "name": "@hua-labs/motion",
  "version": "1.0.0",
  "description": "HUA Labs - Advanced Motion & Animation Hooks for React - Smooth, performant, and customizable animations",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.mjs",
      "require": "./dist/core.js"
    },
    "./page": {
      "types": "./dist/page.d.ts",
      "import": "./dist/page.mjs",
      "require": "./dist/page.js"
    },
    "./element": {
      "types": "./dist/element.d.ts",
      "import": "./dist/element.mjs",
      "require": "./dist/element.js"
    },
    "./scroll": {
      "types": "./dist/scroll.d.ts",
      "import": "./dist/scroll.mjs",
      "require": "./dist/scroll.js"
    },
    "./experiments": {
      "types": "./dist/experiments.d.ts",
      "import": "./dist/experiments.mjs",
      "require": "./dist/experiments.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "node scripts/build-tsc.js",
    "build:watch": "pnpm exec tsup --watch",
    "dev": "pnpm exec tsup --watch",
    "clean": "rm -rf dist",
    "test": "jest",
    "lint": "echo 'Skipping lint for @hua-labs/motion (TypeScript files ignored in root ESLint config)'",
    "type-check": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "analyze": "node scripts/analyze-bundle.js",
    "build:analyze": "pnpm build && pnpm analyze"
  },
  "dependencies": {
    "@hua-labs/motion-core": "workspace:*",
    "@hua-labs/motion-advanced": "workspace:*"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.1.0",
    "@storybook/addon-docs": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-onboarding": "^8.6.14",
    "@storybook/builder-vite": "8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "8.6.14",
    "@storybook/testing-library": "^0.2.2",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "chromatic": "^13.3.4",
    "eslint": "^9.39.1",
    "jest": "^29.0.0",
    "storybook": "8.6.14",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "keywords": [
    "motion",
    "animation",
    "hooks",
    "react",
    "typescript",
    "spring",
    "fade",
    "slide",
    "scale",
    "bounce",
    "pulse",
    "gradient",
    "scroll",
    "gesture",
    "orchestration",
    "korean",
    "english"
  ],
  "author": "HUA Labs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/HUA-Labs/HUA-Labs-public.git"
  },
  "bugs": {
    "url": "https://github.com/HUA-Labs/HUA-Labs-public/issues"
  },
  "homepage": "https://github.com/HUA-Labs/HUA-Labs-public#readme"
}