package:
  name: "@hua-labs/{{shortName}}"
  version: "{{version}}"
  description: "{{description}}"
  license: "MIT"

overview: "{{{overview}}}"

exports:
  main:
    path: "@hua-labs/{{shortName}}"
    provides:
{{#each exports}}
      - name: "{{name}}"
        type: "{{kind}}"
        description: "{{description}}"
{{/each}}
{{#if subpathExports}}

  subpaths:
{{#each subpathExports}}
    - path: "@hua-labs/{{../shortName}}/{{path}}"
      description: "{{description}}"
{{/each}}
{{/if}}

features:
{{#each features}}
  - "{{this}}"
{{/each}}
{{#if related}}

related_packages:
{{#each related}}
  - "@hua-labs/{{this}}"
{{/each}}
{{/if}}

compatibility:
{{#if peerDeps.react}}
  react: "{{peerDeps.react}}"
{{/if}}
  node: ">=20.0.0"
